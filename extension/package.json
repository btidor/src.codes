{
  "name": "distro-source-explorer",
  "displayName": "Distro Source Explorer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:srccodes",
    "onCommand:src-codes-explore"
  ],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "src-codes-explore",
        "title": "Join in Workspace",
        "category": "Distro Source Explorer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile -- --minify",
    "compile": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --sourcemap",
    "watch": "npm run compile -- --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.12",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "esbuild": "^0.12.24",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.4.2",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
