{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https://*.src.codes 'unsafe-inline' 'unsafe-eval' data:;"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'none';"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ],
      "has": [
        {
          "type": "host",
          "value": "(fzf.src.codes|fzf-.*)"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/fzf",
      "has": [
        {
          "type": "host",
          "value": "(fzf.src.codes|fzf-.*)"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "/api/fzf",
      "has": [
        {
          "type": "host",
          "value": "(fzf.src.codes|fzf-.*)"
        }
      ]
    }
  ],
  "github": {
    "silent": true
  }
}
